<%= render 'shared/content_for_pages' %>

<div id="wrapper">

  <main id="content" role="main" class="group">

    <header class="page-header group">
      <div class="hgroup">
        <h1 class="heading-xlarge"><%= t '.cookieTitle' %></h1>
      </div>
    </header>

    <article>

    <p><%= t '.cookie_text1' %></p>
