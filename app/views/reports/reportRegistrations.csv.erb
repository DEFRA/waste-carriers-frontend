<% registrationHeaders = [
  "regIdentifier",
  "companyName",
  "houseNumber",
  "streetLine1",
  "streetLine2",
  "streetLine3",
  "streetLine4",
  "townCity",
  "postcode",
  "firstName",
  "lastName",
  "position",
  "phoneNumber",
  "contactEmail",
  "businessType",
  "otherBusinesses",
  "isMainService",
  "constructionWaste",
  "onlyAMF"
] %>
<% metatDataHeaders = [
  "dateRegistered",
  "dateActivated",
  "status",
  "route"
] %>
<%= CSV.generate_line(registrationHeaders + metatDataHeaders, { :row_sep => nil, :force_quotes => true }).html_safe %>
<% @registrations.each do |registration| %>
  <% row = registration.attributes.values_at(*regMagic) %>
  <% row += registration.metaData.attributes.values_at(*metatDataHeaders) %>
  <%= CSV.generate_line(row, {:row_sep => nil, :force_quotes => true}).html_safe %>
<%end%>