<%= render 'shared/content_for_pages' %>

<div id="wrapper">
  <main id="content" role="main" class="group">

    <% content_for :page_title, createTitle('.renew_title') %>

    <div id="existing-registration" data-journey="waste-carriers-registration:renewal-extension">

      <div class="back-section">
        <ol>
          <li><%= link_to t('form.back_button_label'), start_path %></li>
        </ol>
      </div>

      <header class="text">
        <h1 class="form-title heading-large" id="groupLabel">
          <%= t('.renew_heading_h1') %>
        </h1>
      </header>

      <p class="text"><%= t('.renew_para1') %></p>

      <p class="text"><%= t('.renew_para2') %></p>

      <p class="text"><%= t('.renew_para3') %></p>

      <ul class="list-bullet text">
        <li><%= t('.renew_list_item1') %></li>
        <li><%= t('.renew_list_item2') %></li>
        <li><%= t('.renew_list_item3') %></li>
        <li><%= t('.renew_list_item4') %></li>
        <li><%= t('.renew_list_item5') %></li>
      </ul>

      <h2 class="heading-medium"><%= t('.renew_enforcement_heading_h2') %></h2>

      <p class="text"><%= t('.renew_para4') %></p>

      <p class="text"><%= t('.renew_para5') %></p>

      <h2 class="heading-medium"><%= t('.renew_check_back_heading_h2') %></h2>

      <p class="text"><%= t '.renew_para6' %>
        <a rel='external' href="https://www.gov.uk/government/publications/waste-carrier-broker-and-dealer-registration-renewals-due-after-25-march-2018-rps-208/waste-carrier-broker-and-dealer-registration-renewals-due-after-25-march-2018-rps-208"><%= t '.renew_rps_link' %></a>
      </p>

      <p class="text"><%= t('.renew_para7') %></p>
    </div>
  </main>
</div>
