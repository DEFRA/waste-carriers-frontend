<%= render 'shared/content_for_pages' %>
<main id="content" role="main" class="group">
  <article role="article" class="group">

    <% content_for :page_title, createTitle('directors.directordetails_step.title') %>

    <%= form_for(@registration, :url => newDirectorDetails_path) do |f| %>
      <div class="step"><%= t 'directors.directordetails_step.step' %></div>
      <h1><%= t 'directors.directordetails_step.subtitle' %></h1>

      <%= render 'directordetails_step', f: f%>
    <% end %>
  </article>
</main>