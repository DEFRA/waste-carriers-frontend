<h3>Business or organisation details</h3>
<div class="clr"></div>
<dl class="data">
  <dt>Contact name</dt>
  <dd>
  <%if @registration.title == "Other"%>
  <%= @registration.otherTitle%>
  <%else%>
  <%= @registration.title%>
  <%end%>
  <%= @registration.firstName%> <%= @registration.lastName%>
  </dd>
  <dt>Phone number</dt>
  <dd><%= @registration.phoneNumber%></dd>
</dl>
<dl class="data">
  <dt>Address</dt>
  <dd><%= @registration.houseNumber %> <%= @registration.streetLine1 %><br /><%= @registration.streetLine2 %> <%= @registration.townCity%> <%= @registration.postcode%></dd>
</dl>

<%if @registration.assisted_digital? %>
<dl class="data">
  <dt>Access code</dt>
  <dd><%= @registration.access_code %> </dd>
</dl>
<%end%>

<div class="clr"></div>

<div class="clr"></div>
