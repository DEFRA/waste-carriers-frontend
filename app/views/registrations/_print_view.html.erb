
  <% if @registration.errors.any? %>
    <div id="error_explanation" class="validation-summary">
      <h2><%= pluralize(@registration.errors.count, "error") %> prohibited this registration from being saved:</h2>

      <ul>
      <% @registration.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<!-- WF 4.3.1 -->
<div id="page1">


<fieldset>
<legend><%= t 'registrations.print.detailsHeading'%></legend>

<div class="form-group<%if @registration.errors[:businessType].any?%> validation<%end%>">
<%= f.label :businessType, "Business or organisation type", class: 'form-label' %>
<%= f.text_field :businessType, :readOnly => true, :size => 100 %>
</div>

<div id="orgTypeQuestions" class="js-hidden">

<div class="form-group<%if @registration.errors[:companyName].any?%> validation<%end%>">
<%= f.label :companyName, "Business, organisation or trading name", class: 'form-label' %>
<%= f.text_field :companyName, :readOnly => true, :size => 100 %>
</div>

<div id="indType-wrapper" class="group<%if @registration.errors[:individualsType ].any?%> validation<%end%>">
<%= f.label :individualsType, "Organisation type", class: 'form-label' %>
<%= f.text_field :individualsType, :readOnly => true, :size => 100 %>
</div>

<div id="publicBodyType-wrapper" class="group<%if @registration.errors[:publicBodyType].any?%> validation<%end%>">
<%= f.label :publicBodyType, "Type of public body", class: 'form-label' %>
<%= f.text_field :publicBodyType, :readOnly => true, :size => 100 %>
</div>

<div id="publicBodyOther-wrapper" class="group<%if @registration.errors[:publicBodyTypeOther].any?%> validation<%end%>">
<%= f.label :publicBodyTypeOther, "Other type of public body", class: 'form-label' %>
<%= f.text_field :publicBodyTypeOther, :readOnly => true, :size => 100 %>
</div>
</div>

</fieldset>


</div>

<!-- WF 4.3.2 -->
<div id="page2">

<h2><%= t 'registrations.print.contactHeading'%></h2>

<fieldset>
<legend><%= t 'registrations.print.addressHeading'%></legend>


<div id="addressSearch">
<div class="form-group<%if @registration.errors[:houseNumber].any?%> validation<%end%>">
<%= f.label :houseNumber, "House number", class: 'form-label' %>
<%= f.text_field :houseNumber, :readOnly => true, :size => 100 %>
</div>

<div id="addressManual" style="display:none">
<div class="form-group<%if @registration.errors[:streetLine1].any?%> validation<%end%>">
<%= f.label :streetLine1, "Street", class: 'form-label' %>
<%= f.text_field :streetLine1, :readOnly => true, :size => 100 %>
</div>
<div class="form-group<%if @registration.errors[:streetLine2].any?%> validation<%end%>">
<%= f.label :streetLine2, :class=>"form-group offscreen" %>
<%= f.text_field :streetLine2, :readOnly => true, :size => 100 %>
</div>
<div class="form-group<%if @registration.errors[:townCity].any?%> validation<%end%>">
<%= f.label :townCity, "Town/City", class: 'form-label' %>
<%= f.text_field :townCity, :readOnly => true, :size => 100 %>
</div>
</div>

<div class="form-group<%if @registration.errors[:postcode].any?%> validation<%end%>">
    <%= f.label :postcode, "Postcode", class: 'form-label' %>
    <%= f.text_field :postcode, :readOnly => true, :size => 100 %>
</div>

<div class="action group">
<!--<input name="findAddress" class="button-secondary" type="submit" value="Find address"/>-->
</div>
</div>

<div id="addressSearchResults" style="display:none">
<div class="form-group">
<label for="addresses"><span id="resultCount">5</span> Addresses found</label>
<select id="addresses">
<option>Please select an address..</option>
</select>
</div>
<a href="#" id="manualAddressLink">I cannot find my address</a>
</div>

<div id="selectedAddress" style="display:none">
<div id="addressDisplay">
</div>
<div class="action group">
<input name="changeAddress" class="button-secondary" type="submit" value="Change address"/>
</div>
</div>

<%= f.hidden_field :uprn %>
<%= f.hidden_field :address %>
</fieldset>

<fieldset>
<legend>Contact information</legend>

<div class="form-group<%if @registration.errors[:firstName].any?%> validation<%end%>">
<%= f.label :title, class: 'form-label' %>
<%= f.text_field :title, :readOnly => true, :size => 100 %>
</div>
<div class="form-group<%if @registration.errors[:firstName].any?%> validation<%end%>">
    <%= f.label :firstName, "First name", class: 'form-label' %>
    <%= f.text_field :firstName, :readOnly => true, :size => 100 %>
</div>
<div class="form-group<%if @registration.errors[:lastName].any?%> validation<%end%>">
    <%= f.label :lastName, "Last name", class: 'form-label' %>
    <%= f.text_field :lastName, :readOnly => true, :size => 100 %>
</div>
<div class="form-group<%if @registration.errors[:phoneNumber].any?%> validation<%end%>">
    <%= f.label :phoneNumber, "Phone number", class: 'form-label' %>
    <%= f.telephone_field :phoneNumber, :readOnly => true, :size => 100 %>
</div>
<div class="form-group<%if @registration.errors[:email].any?%> validation<%end%>">
    <%= f.label :email, "Email address", class: 'form-label' %>
    <%= f.text_field :email, :readOnly => true, :size => 100 %>
</div>
</fieldset>

</div>

<div class="action group">
	<input name="back" class="button-secondary" type="submit" value="Back"/>
</div>