<%= render 'shared/content_for_pages' %>

<main id="content" role="main" class="group">
  <article role="article" class="group">
  	 <section>
      <div class="inner">
      <div class="col-1">

    <% content_for :page_title, createTitle('registrations.noregistration_step.title') %>
    <%= form_for(@registration, :url => newNoRegistration_path) do |f| %>

      <ol>
      	<li class="step"><%= t 'registrations.noregistration_step.step' %></li>
      	<li class="subtitle"><%= t 'registrations.noregistration_step.subtitle' %></li>
	  </ol>
      <%= render "noregistration_step", :f => f %>

    <% end %>

        </div>
  		</div>
	</section>
  </article>
</main>
