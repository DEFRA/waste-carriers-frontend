<%= render 'shared/content_for_pages' %>

<main id="content" role="main" class="group">
  <article role="article" class="group">
  	 <section>
      	<div class="inner">
      		<div class="col-1">
    			<% content_for :page_title, createTitle('registrations.registrationtype_step.title') %>
    			<%= form_for(@registration, :url => newRegistrationType_path) do |f| %>
      			<%= render 'back_link' %>
      			<%= render "registrationtype_step", :f => f %>
    		<% end %>
    		</div>
		</div>
	</section>
  </article>
</main>
