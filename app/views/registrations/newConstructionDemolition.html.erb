<%= render 'shared/content_for_pages' %>

<main id="content" role="main" class="group">
  <article role="article" class="group">

    <% content_for :page_title, createTitle('registrations.constructiondemolition_step.title') %>
    <%= form_for(@registration, :url => newConstructionDemolition_path) do |f| %>

      <p class="back_link"><%= render 'back_link' %></p>
      <div class="step"><%= t 'registrations.constructiondemolition_step.step' %></div>

      <%= render "constructiondemolition_step", :f => f %>

    <% end %>

  </article>
</main>
