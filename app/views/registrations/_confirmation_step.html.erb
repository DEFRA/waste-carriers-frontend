<%= render "errors" %>

<!-- WF 4.3.3 -->
<div id="page9"  data-journey="waste-carriers-registration:confirmation">

  <h2><%= t '.smart_answers_heading' %></h2>
    <%= render 'smartanswers_display' %>
    <%= link_to t('form.edit_these_details'), newBusinessType_path %>

  <hr>

  <h2><%= t 'registrations.address_entry.businessAddress' %></h2>
    <%= render 'address_display' %>
    <%= link_to t('form.edit_these_details'), newBusinessDetails_path %>
  
  <hr>

  <h2><%= t 'registrations.contactdetails_step.title' %></h2>
    <%= render 'contactdetails_display' %>
    <%= link_to t('form.edit_these_details'), newContact_path %>
  
  <hr>

  <h2><%= t '.declaration_heading' %></h2>
  <div class="form-group<%if @registration.errors[:declaration].any?%> validation<%end%>">
    <%= validation_for(@registration, :declaration) %>
    <div class='text panel-indent'><%= f.check_box :declaration%><%= t '.declaration_text' %></div>
  </div>

  <hr>

  <h2><%= t '.data_protection_heading' %></h2>
    <p><%= t '.data_protection_text1' %></p> 

      <%= link_to t('registrations.dataProtection.subtitle'), dataProtection_path, :target => "_blank" %>

  </br>  
      <%= link_to t('.privacy_policy'), Rails.configuration.waste_exemplar_eaPrivacyCookies_url, :target => "_blank" %>
   

</div>

<%= render 'wizard_links' %>
