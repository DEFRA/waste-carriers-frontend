<%= render 'shared/content_for_pages' %>
<div id="wrapper">
  <div class="back-section">
    <ol>
      <li><%= render 'back_link' %></li>
    </ol>
  </div>
<main id="content" role="main" class="group">
  <article role="article" class="group">
  	<section>
      	<div class="inner">
      		<div class="col-1">
      		<% content_for :page_title, createTitle('registrations.serviceprovided_step.title') %>
      		<%= form_for(@registration, :url => newServiceProvided_path) do |f| %>
      		<%= render "serviceprovided_step", :f => f %>
    		<% end %>
    		</div>
		</div>
	</section>
  </article>
</main>
</div>