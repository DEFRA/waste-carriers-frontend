<%= render 'shared/content_for_pages' %>

<div id="wrapper">

<main id="content" role="main" class="group">

<div class="back-section">
    <ol>
      <li><%= link_to t('agencyUsers.back'), agency_users_path %></li>
    </ol>
</div>

<header class="page-header group">
  <div class="hgroup">
    <% if !notice.nil? %>
      <div id="note_explanation" class="note-summary">
        <h1 class="heading-large"><%= notice%></h1>
      </div>
    <% end %>
  </div>
</header>

<div class="grid-wrapper">

<div class="grid-2-3">
  <div class="inner-block">
    <p><strong><%=t 'agencyUsers.name'%>:</strong><%= @agency_user.email %></p>
    <a><%= link_to t('agencyUsers.edit'), edit_agency_user_path(@agency_user) %></a>
    <a><%= link_to t('agencyUsers.delete'), confirm_delete_agency_user_path(@agency_user) %></a>
  </div>
</div><!-- end grid-2-3 -->

  <div class="grid-1-3">
    <div class="inner-block related">
      <div class="inner group related-subsection">
        <h2 class="heading-small">Current user</h2>
          <nav aria-labelledby="parent-subsection" role="navigation">
            <ul>
              <li><%= render "shared/current_user" %></li>
            </ul>
          </nav>
        </div>
      </div>
    </div><!-- end grid 1-3-->

  </div><!-- end grid-wrapper -->

  </main><!-- end main -->
</div><!-- end wrapper -->
