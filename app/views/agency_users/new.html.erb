<%= render 'shared/content_for_pages' %>

<div id="wrapper">

<main id="content" role="main" class="group">

<div class="back-section">
    <ol>
      <li><%= link_to t('agencyUsers.back'), agency_users_path %></li>
    </ol>
</div>

<header class="page-header group">
    <div class="hgroup">
        <h1 class="heading-large"><%=t 'agencyUsers.newLabel'%></h1>
    </div>
</header>

	<div class="grid-wrapper">

    <div class="grid-2-3">
      <div class="inner-block">

       <%= render 'form' %>

      </div><!--end inner-block -->
	   </div><!--end grid-2-3 -->

      <div class="grid-1-3">
        <div class="inner-block related">
          <div class="inner group related-subsection">
            <h2 class="heading-small"><%= t 'registrations.form.currentUser' %></h2>
              <nav aria-labelledby="parent-subsection" role="navigation">
                <ul>
                  <li><%= render "shared/current_user" %></li>
                </ul>
              </nav>
            </div>
          </div>
        </div><!-- end grid 1-3-->
    </div><!-- end grid-wrapper -->

  </main><!-- end main -->
</div><!-- end wrapper -->