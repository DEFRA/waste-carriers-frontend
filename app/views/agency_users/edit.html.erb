<%= render 'shared/content_for_pages' %>
<%#= render "shared/current_user" %>

<div id="wrapper">

<main id="content" role="main" class="group">

<div class="back-section">
    <ol>
      <li><%# link_to t('agencyUsers.show'), @agency_user %>
          <%= link_to t('agencyUsers.back'), agency_users_path %>
      </li>
    </ol>
</div>

<header class="page-header group">
  <div class="hgroup">
    <h1 class="heading-large"><%=t 'agencyUsers.editHeading'%></h1>
  </div>
</header>

<div class="grid-wrapper">

  <div class="grid-2-3">
    <div class="inner-block">

      <%= render 'form' %>

    </div><!-- end inner-block -->
  </div><!-- end grid-2-3 -->

  <div class="grid-1-3">
  <div class="inner-block related">
    <div class="inner group related-subsection">
      <h2 class="heading-small">Current user</h2>
         <%= render "shared/current_user" %>
      </div>
    </div>
  </div><!-- end grid 1-3-->

</div><!-- end grid-wrapper -->

</main><!-- end main -->
</div><!-- end wrapper -->