<% if user_signed_in? %>
  <div class="current_user external">
  Signed in as <strong><%= current_user.email %></strong><br/>
  <%= link_to "Change password", edit_user_registration_path, :class => 'navbar-link' %><br/>
  <%= button_to('Sign out', destroy_user_session_path, :method => :delete, :class => 'navbar-link') %>
  </div>
  <% elsif agency_user_signed_in? %>
  <div class="current_user internal">
  Signed in as agency user <strong><%= current_agency_user.email %></strong><br/>
  <%= link_to "Change password", edit_agency_user_registration_path, :class => 'navbar-link' %><br/>
  <%= button_to('Sign out', destroy_agency_user_session_path, :method => :delete, :class => 'navbar-link') %>
  </div>
  <% elsif admin_signed_in? %>
  <div class="current_user admin">
  Signed in as admin <strong><%= current_admin.email %></strong><br/>
  <%= link_to "Change password", edit_admin_registration_path, :class => 'navbar-link' %><br/> 
  <%= button_to('Sign out', destroy_admin_session_path, :method => :delete, :class => 'navbar-link') %>
  </div>
<% else %>
  <div class="current_user signin">
  <%#= link_to "Sign up", new_user_registration_path, :class => 'navbar-link'  %> 
  <% if current_page?(new_admin_session_path) %>
  <%= link_to "Sign in", new_admin_session_path, :class => 'navbar-link'  %>
  <% elsif current_page?(registrations_path) || current_page?(new_agency_user_session_path) %>
  <%= link_to "Sign in", new_agency_user_session_path, :class => 'navbar-link'  %>
  <% else %>
  <%= link_to "Sign in", new_user_session_path, :class => 'navbar-link'  %>
  <% end %>
  </div>
<% end %>

