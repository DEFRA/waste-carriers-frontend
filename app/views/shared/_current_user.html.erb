<% if user_signed_in? %>
  <div class="current_user external">
  <p><%= t('.signed_in_as')%> <strong><%= current_user.email %></strong></p>
  <p><%= link_to t('.change_password'), edit_user_registration_path %></p>
  <%= button_to t('.sign_out'), destroy_user_session_path, :method => :delete, :class => 'button', :id => 'signout_button' %>
  </div>
  <% elsif agency_user_signed_in? %>
  <div class="current_user internal">
  <p><%= t('.signed_in_as_agency_user')%> <strong><%= current_agency_user.email %></strong></p>
  <p><%= link_to t('.change_password'), edit_agency_user_registration_path, :class => 'navbar-link' %></p>
  <p><%= link_to t('.report_export'), registrations_search_path %></p>
  <%= button_to t('.sign_out'), destroy_agency_user_session_path, :method => :delete, :class => 'button', :id => 'signout_button' %>
  </div>
  <% elsif admin_signed_in? %>
  <div class="current_user admin">
  <p><%= t('.signed_in_as_admin')%> <strong><%= current_admin.email %></strong></p>
  <p><%= link_to t('.change_password'), edit_admin_registration_path %></p>
  <%= button_to t('.sign_out'), destroy_admin_session_path, :method => :delete, :class => 'button', :id => 'signout_button' %>
  </div>
<% else %>
  <div class="current_user signin">
  <%#= link_to t(".sign_up"), new_user_registration_path, :class => 'navbar-link'  %>
  <%# if current_page?(new_admin_session_path) %>
  <%# link_to t('.sign_in'), new_admin_session_path, :class => 'navbar-link'  %>
  <%# elsif current_page?(registrations_path) || current_page?(new_agency_user_session_path) %>
  <%# link_to t('.sign_in'), new_agency_user_session_path, :class => 'navbar-link'  %>
  <%# else %>
  <%# link_to t('.sign_in'), new_user_session_path, :class => 'navbar-link'  %>
  <%# end %>
  </div>
<% end %>

