<% if user_signed_in? %>
  <div class="current_user external">
  Logged in as <strong><%= current_user.email %></strong><br/>
  <%= link_to "Change Password", edit_user_registration_path, :class => 'navbar-link' %><br/>
  <%= link_to "Logout", destroy_user_session_path, :method => :delete, :class => 'navbar-link'  %>
  </div>
  <% elsif agency_user_signed_in? %>
  <div class="current_user internal">
  Logged in as agency user <strong><%= current_agency_user.email %></strong><br/>
  <%= link_to "Change Password", edit_agency_user_registration_path, :class => 'navbar-link' %><br/>
  <%= link_to "Logout", destroy_agency_user_session_path, :method => :delete, :class => 'navbar-link'  %>  
  </div>
  <% elsif admin_signed_in? %>
  <div class="current_user admin">
  Logged in as admin <strong><%= current_admin.email %></strong><br/>
  <%= link_to "Change Password", edit_admin_registration_path, :class => 'navbar-link' %><br/>
  <%= link_to "Logout", destroy_admin_session_path, :method => :delete, :class => 'navbar-link'  %>  
  </div>
<% else %>
  <div class="current_user signin">
  <%#= link_to "Sign up", new_user_registration_path, :class => 'navbar-link'  %> 
  <%= link_to "Login", new_user_session_path, :class => 'navbar-link'  %>
  </div>
<% end %>

