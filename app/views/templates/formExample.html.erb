<%= render 'shared/content_for_pages' %>
<div id="wrapper">

  <main id="content" role="main" class="group">

    <div class="back-section">
        <ol>
          <li><%= render 'back_link' %></li>
        </ol>
    </div>

    <header class="page-header group">
      <div class="hgroup">
          <h1 class="heading-xlarge">Example styles page</h1>
      </div>
    </header>

    <div class="grid-wrapper">
      <!-- WF 4.3.2 -->
    <div id="page0"  data-journey="waste-carriers-registration:example">
    <% content_for :page_title, createTitle('.title') %>

      <div class="grid-2-3">
        <div class="inner-block">
        <%= render "errors" %>
        <%= form_for(@registration, :url => formExample_path) do |f| %>
      	<%= render "formExample_step", f: f %>
        <span class="next_link"><%= render 'next_link' %></span>
        <% end %>
      </div>
    </div>

      <div class="grid-1-3">
        <div class="inner-block">
        <div class="related">
          <div class="inner group related-subsection">
            <h2 class="heading-small">This is a sidebar</h2>
              <nav aria-labelledby="parent-subsection" role="navigation">
                <ul>
                  <li><p>This is example content</p></li>
                  <li><p>This is example content</p></li>
                  <li><p>This is example content</p></li>
                </ul>
              </nav>
          </div>
        </div><!--inner-block end-->
      </div><!--grid-1-3 end-->
    </div><!--grid-wrapper end-->

    </div><!--page id end-->
    </div><!--grid-wrapper end-->


  </main><!--main end-->
</div><!--wrapper end-->


